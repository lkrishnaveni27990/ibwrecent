
 <!-- video section -->
	<section class="bg-light">
		<div class="text-center   col-12" style="align-items: center;">
			<h1>Our Videos</h1>
			<br>
			<video id="slider" class="animate__animated animate__fadeInUp animate__slow"
				style="border-radius: 1rem;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);width:100%;height:30rem;opacity:70;"
				controls autoplay muted loop>
				<source src="video/showerhead.mp4" type="video/mp4">
			</video>

			<ul class="navigation text-center col-12">
				{{#each videos}}

				<li onclick="videoselector('../slider-videos/{{this._id}}.mp4')" >
				<video style="width:150px;height:100px;border-radius:.5rem"src="../slider-videos/{{this._id}}.mp4"></video>
				</li>

				{{/each}}
				
			</ul>

		</div>
	</section>
	<!-- end of video section -->
	<br>
	
	
